package me.ywork.salary.entity;

import me.ywork.base.entity.Entity;

public class SalStaffBaseInfoEntity extends Entity {
	/**
	 * 员工的id
	 */
	private String staffId;
	/**
	 * 员工的姓名
	 */
	private String staffName;
	/**
	 * 员工所属企业的Id
	 */
	private String corpId;
	/**
	 * 部门的名字
	 */
	private String deptName;
	/**
	 * 计薪规则
	 */
	private Short salRule;

	public String getStaffId() {
		return staffId;
	}

	public void setStaffId(String staffId) {
		this.staffId = staffId;
	}

	public String getStaffName() {
		return staffName;
	}

	public void setStaffName(String staffName) {
		this.staffName = staffName;
	}

	public String getCorpId() {
		return corpId;
	}

	public void setCorpId(String corpId) {
		this.corpId = corpId;
	}

	public String getDeptName() {
		return deptName;
	}

	public void setDeptName(String deptName) {
		this.deptName = deptName;
	}

	public Short getSalRule() {
		return salRule;
	}

	public void setSalRule(Short salRule) {
		this.salRule = salRule;
	}

}
